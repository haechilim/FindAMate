<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.findamate">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.FindAMate">
        <activity
            android:name=".activity.PopupMatchingSettingActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".activity.LogActivity" />
        <activity
            android:name=".activity.PopupStudentSettingActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".activity.PopupClassSettingActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".activity.SimulationActivity" />
        <activity android:name=".activity.WaitingActivity" />
        <activity android:name=".activity.SignupActivity" />
        <activity android:name=".activity.MatchingActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>